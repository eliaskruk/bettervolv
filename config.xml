<?xml version="1.0" encoding="UTF-8" ?><widget xmlns = "http://www.w3.org/ns/widgets"        xmlns:gap   = "http://phonegap.com/ns/1.0"        xmlns:android   = "http://schemas.android.com/apk/res/android"        id          = "com.betterpixel.volvo"        version     = "1.0.0"        versionCode = "10" >    <!-- versionCode is optional and Android only -->    <preference name="phonegap-version" value="3.7.0" />    <name>Volvo</name>    <description>        Volvo    </description>    <author href="" email="elias.kruk@gmail.com">Elias Kruk</author>    <preference name="orientation" value="landscape" />    <preference name="target-device" value="universal" />    <preference name="fullscreen" value="true" />    <preference name="webviewbounce" value="true" />    <preference name="prerendered-icon" value="true" />    <preference name="stay-in-webview" value="true" />    <preference name="detect-data-types" value="true" />    <preference name="exit-on-suspend" value="true" />    <preference name="show-splash-screen-spinner" value="true" />    <preference name="auto-hide-splash-screen" value="true" />    <preference name="disable-cursor" value="true" />    <preference name="android-minSdkVersion" value="7" />    <preference name="android-targetSdkVersion" value="19" />    <preference name="android-installLocation" value="auto" />    <preference name="splash-screen-duration" value="1" />    <preference name="load-url-timeout" value="0" />    <preference name="android-windowSoftInputMode" value="adjustResize" />    <preference name="AndroidPersistentFileLocation" value="Compatibility" />    <preference name="AndroidExtraFilesystems" value="files,files-external,documents,sdcard,cache,cache-external,root" />    <preference name="showmessageinnotification" value="true" />    <preference name="StatusBarOverlaysWebView" value="true" />        <gap:platform name="ios" />    <gap:platform name="android" />        <feature name="http://api.phonegap.com/1.0/media"/>    <feature name="http://api.phonegap.com/1.0/network"/>                <gap:plugin name="org.apache.cordova.network-information"  />    <gap:plugin name="org.apache.cordova.console" version="0.2.7" />    <gap:plugin name="org.apache.cordova.dialogs" version="0.2.6" />    <gap:plugin name="org.apache.cordova.splashscreen" version="0.2.7" />    <gap:plugin name="net.yoik.cordova.plugins.screenorientation" version="1.1.1" />    <gap:plugin name="org.apache.cordova.device"  />    <gap:plugin name="com.jamiestarke.webviewdebug" version="1.0.8" />    <gap:plugin name="net.tunts.webintent"  version="0.2.1" />    <gap:plugin name="org.li8.inappbrowser" version="0.1" />    <gap:plugin name="org.apache.cordova.file" />    <gap:plugin name="org.apache.cordova.file-transfer" />    <gap:plugin name="com.ionic.keyboard" version="1.0.3" />        <icon src="imagenes/icon.png" gap:platform="android" gap:density="ldpi" />    <icon src="imagenes/icon.png" gap:platform="android" gap:density="mdpi" />    <icon src="imagenes/icon.png" gap:platform="android" gap:density="hdpi" />    <icon src="imagenes/icon.png" gap:platform="android" gap:density="xhdpi" />        <gap:splash src="imagenes/bg.jpg" width="640" height="960" />    <gap:splash src="imagenes/bg.jpg" width="480" height="800 " />    <gap:splash src="imagenes/bg.jpg" gap:platform="android" gap:density="hdpi" />    <gap:splash src="imagenes/bg.jpg" gap:platform="android" gap:density="xhdpi" />        <access origin="*" />    </widget>