<?xml version="1.0" encoding="UTF-8" ?><widget xmlns = "http://www.w3.org/ns/widgets"        xmlns:gap   = "http://phonegap.com/ns/1.0"        xmlns:android   = "http://schemas.android.com/apk/res/android"        id          = "app.betterpixel.volvo"        version     = "1.0.0"        versionCode = "10" >    <!-- versionCode is optional and Android only -->    <preference name="phonegap-version" value="3.7.0" />    <name>Volvo</name>    <description>        Volvo    </description>    <author href="" email="elias.kruk@gmail.com">Elias Kruk</author>    <preference name="orientation" value="landscape" />    <preference name="target-device" value="universal" />    <preference name="fullscreen" value="true" />    <preference name="webviewbounce" value="true" />    <preference name="prerendered-icon" value="true" />    <preference name="stay-in-webview" value="true" />    <preference name="detect-data-types" value="true" />    <preference name="exit-on-suspend" value="true" />    <preference name="show-splash-screen-spinner" value="true" />    <preference name="auto-hide-splash-screen" value="true" />    <preference name="disable-cursor" value="true" />    <preference name="android-minSdkVersion" value="7" />    <preference name="android-targetSdkVersion" value="19" />    <preference name="android-installLocation" value="auto" />    <preference name="splash-screen-duration" value="1" />    <preference name="load-url-timeout" value="0" />    <preference name="android-windowSoftInputMode" value="adjustResize" />                    <icon src="imagenes/icon.png" gap:platform="android" gap:density="ldpi" />    <icon src="imagenes/icon.png" gap:platform="android" gap:density="mdpi" />    <icon src="imagenes/icon.png" gap:platform="android" gap:density="hdpi" />    <icon src="imagenes/icon.png" gap:platform="android" gap:density="xhdpi" />            <gap:plugin name="org.apache.cordova.console" version="0.2.7" />    <gap:plugin name="org.apache.cordova.network-information" version="0.2.7" />    <gap:plugin name="org.apache.cordova.dialogs" version="0.2.6" />    <gap:plugin name="org.apache.cordova.splashscreen" version="0.2.7" />    <preference name="showmessageinnotification" value="true" />    <preference name="defaultnotificationmessage" value="Hay Ekipo" />        <gap:plugin name="org.apache.cordova.file" />    <gap:plugin name="org.apache.cordova.file-transfer" />        <gap:config-file platform="android" parent="/manifest">        <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />    </gap:config-file>        </widget>